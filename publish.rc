#!/usr/bin/env rc
# vim:set ft=text ts=4 sw=4:

cd /var/www/henry.precheur.org/scratchpad/

fn rfc3339 {
    date +%FT%T%z | sed 's/\([+-][01][0-9]\)\([0-9][0-9]\)$/\1:\2/'
}

dir=.log
mkdir $dir

{
    rfc3339
    echo
    cat
    echo 
} | tee -a log $dir/`{rfc3339}

python html.py < log > index.html
python atom.py < log > feed.atom
